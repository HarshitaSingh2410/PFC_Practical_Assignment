#include <stdio.h>
int main() {
    int i, j;
    int rows = 7, cols = 5;
    for (i = 0; i < rows; i++) {  
        for (j = 0; j < cols; j++) {      
            if (j == 0 ||               
                (i == 0 && j < cols-1) ||   
                (i == 3 && j < cols-1) ||  
                (j == cols-1 && i > 0 && i < 3) ||  
                (i - j == 3))               
            {
                printf("*");
            } else {
                printf(" ");
            }
        }
        printf("\n");
    }
    return 0;
}
/*OUTPUT
**** 
*   *
*   *
****
**
* *
*  *  */
