#include <stdio.h>
int main() {
    int i, j;
    for (i = 1; i <= 5; i++) {
        for (j = 1; j <= 9; j++) {
            if (j == 1 || j == 9 || 
                (i == 3 && (j == 3 || j == 7)) ||
                (i == 4 && (j == 4 || j == 6)) ||
                (i == 5 && j == 5))
            {
                printf("*");
            } else {
                printf(" ");
            }
        }
        printf("\n");
    }
    return 0;
}
/*OUTPUT
*       *
*       *
* *   * *
*  * *  *
*   *   *  */
